{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "description": "Message type for `instantiate` entry_point",
  "type": "object",
  "required": [
    "channel",
    "denom",
    "dest_address",
    "min_amount"
  ],
  "properties": {
    "channel": {
      "type": "string"
    },
    "denom": {
      "type": "string"
    },
    "dest_address": {
      "type": "string"
    },
    "min_amount": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "additionalProperties": false
}
